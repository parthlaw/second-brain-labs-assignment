datasource db{
  url= env("DATABASE_URL")
  provider = "postgresql"
}

generator client{
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String?
  createdAt DateTime @default(now())
  email     String   @unique
  password  String
}

model FileUploads {
  id        Int      @id @default(autoincrement())
  name      String?
  path      String
  generated_vector    String
}
